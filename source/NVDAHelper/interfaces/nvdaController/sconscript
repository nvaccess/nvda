Import('env')

headerFile,clientSourceFile,serverSourceFile=env.MSRPCStubs(
	source=['nvdaController.idl'],
	MIDLFLAGS=env['MIDLFLAGS']+['/prefix','all','nvdaController_','/acf',File('nvdaController.acf')],
)
clientObjFile=env.Object(clientSourceFile)
serverObjFile=Object(serverSourceFile)

Return(['headerFile','clientObjFile','serverObjFile'])
