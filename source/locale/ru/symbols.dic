complexSymbols:
# identifier	regexp
# Sentence endings.
. sentence ending	(?<=[^\s.])\.(?=[\"'”’)\s]|$)
! sentence ending	(?<=[^\s!])\!(?=[\"'”’)\s]|$)
? sentence ending	(?<=[^\s?])\?(?=[\"'”’)\s]|$)
# Phrase endings.
; phrase ending	(?<=[^\s;]);(?=\s|$)
: phrase ending	(?<=[^\s:]):(?=\s|$)
# Others
decimal point	(?<![^\d -])\.(?=\d)
decimal comma	(?<![^\d -]),(?=\d)
in-word '	(?<=[^\W_])['’]
negative number	(?<!\w)[-−]{1}(?=[$£€¥.]?\d)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	точка	all	always	# . в конце предложения
! sentence ending	восклицательный знак	all	always	# ! в конце предложения
? sentence ending	вопросительный знак	all	always	# ? в конце предложения
; phrase ending	точка с запятой	most	always	# ; в конце фразы
: phrase ending	двоеточие	most	always	# : в конце фразы
decimal point		none	always	# десятичная точка
decimal comma		none	always	# десятичная запятая
in-word '	апостроф	all	norep	# ' в слове
negative number	минус	none	norep	# знак отрицательного числа

