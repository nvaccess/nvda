#. %s will be replaced with the destination directory.
#, python-format
msgid "Successfully created a portable copy of NVDA at %s"
msgstr "Prenosná verzia bola úspešne vytvorená v priečinku %s"

#. Translators: The title of the NVDA log viewer window.
msgid "NVDA Log Viewer"
msgstr "Zobrazovač Logu NVDA"

#. Translators: The label for a menu item in NVDA log viewer to refresh log messages.
msgid "Refresh\tF5"
msgstr "Obnoviť\tF5"

#. Translators: The label for a menu item in NVDA log viewer to save log file.
msgid "Save &as...\tCtrl+S"
msgstr "uložiť &ako...\tCtrl+S"

#. Translators: The title of a menu in NVDA Log Viewer.
msgid "Log"
msgstr "Log"

#. Translators: Label of a menu item in NVDA Log Viewer.
msgid "Save As"
msgstr "Uložiť Ako"

#. Translators: Dialog text presented when NVDA cannot save a log file.
#, python-format
msgid "Error saving log: %s"
msgstr "Chyba pri ukladaní logu: \"%s\"."

#. Translators: A cancel button on a message dialog.
#. Translators: The cancel button on a NVDA dialog. This button will discard any changes and dismiss the dialog.
msgid "Cancel"
msgstr "Zrušiť"

#. Translators: The Apply button on a NVDA dialog. This button will accept any changes but will not dismiss the dialog.
msgid "Apply"
msgstr "Použiť"

#. Translators: The label for the list of categories in a multi category settings dialog.
msgid "&Categories:"
msgstr "&Kategórie:"

#. Translators: This is the label for the general settings panel.
msgid "General"
msgstr "Všeobecné"

#. Translators: One of the log levels of NVDA (the disabled mode turns off logging completely).
msgid "disabled"
msgstr "zakázaný"

#. Translators: One of the log levels of NVDA (the info mode shows info as NVDA runs).
msgid "info"
msgstr "Info"

#. Translators: One of the log levels of NVDA (the debug warning shows debugging messages and warnings as NVDA runs).
msgid "debug warning"
